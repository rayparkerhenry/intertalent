{
  "name": "talent-showcase-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3000",
    "dev:port": "next dev -p",
    "build": "next build && npm run copy-static",
    "copy-static": "node scripts/copy-standalone-files.js",
    "build:windows": "next build && xcopy public .next\\standalone\\public /E /I /Y && xcopy .next\\static .next\\standalone\\.next\\static /E /I /Y",
    "start": "node server.js",
    "start:standalone": "node .next/standalone/server.js",
    "start:next": "next start",
    "lint": "eslint",
    "test:db": "npx dotenv-cli -e .env.local -- npx tsx src/lib/db/test-connection.ts",
    "test:azure": "npx dotenv-cli -e .env.local -- npx tsx src/lib/db/test-azure-sql.ts",
    "import:azure": "npx dotenv-cli -e .env.local -- npx tsx scripts/import-to-azure-sql.ts",
    "import:ray-test": "npx dotenv-cli -e .env.local -- npx tsx scripts/import-to-ray-test.ts",
    "import:ray-test-accurate": "npx dotenv-cli -e .env.local -- npx tsx scripts/import-to-ray-test-accurate.ts",
    "import:location-emails": "npx dotenv-cli -e .env.local -- npx tsx scripts/import-location-emails.ts",
    "test:geo": "npx dotenv-cli -e .env.local -- npx tsx scripts/test-geo-performance.ts",
    "test:zip-api": "npx tsx scripts/test-zip-api.ts",
    "test:radius-search": "npx dotenv-cli -e .env.local -- npx tsx scripts/test-radius-search.ts",
    "test:helpers": "npx dotenv-cli -e .env.local -- npx tsx src/lib/db/test-helpers.ts",
    "test:sync": "npx dotenv-cli -e .env.local -- npx tsx src/lib/sync/test-sync.ts",
    "seed": "npx dotenv-cli -e .env.local -- npx tsx src/lib/db/seed.ts",
    "sync:now": "npx dotenv-cli -e .env.local -- npx tsx src/lib/cron/run-sync.ts",
    "cron:start": "npx dotenv-cli -e .env.local -- npx tsx src/lib/cron/start-cron.ts"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.81.1",
    "csv-parse": "^6.1.0",
    "date-fns": "^4.1.0",
    "lodash": "^4.17.21",
    "mssql": "^12.1.1",
    "next": "16.0.1",
    "node-cron": "^4.2.1",
    "nodemailer": "^7.0.11",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/lodash": "^4.17.20",
    "@types/mssql": "^9.1.8",
    "@types/node": "^20",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^7.0.4",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "dotenv-cli": "^11.0.0",
    "eslint": "^9",
    "eslint-config-next": "16.0.1",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.6.2",
    "tailwindcss": "^4",
    "tsx": "^4.20.6",
    "typescript": "^5",
    "xlsx": "^0.18.5"
  }
}
